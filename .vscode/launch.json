{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "使用 Deepseek API",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "pnpm", // 使用 pnpm 作为运行时
      "args": [
        "dev"
      ], // 执行 dev 命令
      "cwd": "${workspaceFolder}/deepseek_basic", // 项目1路径
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
